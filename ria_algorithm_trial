CREATE TABLE [Participants for Invent the Summer].[Identify]
(
[Identify] [int] NOT NULL IDENTITY 
[Firstname_id] [varchar] NOT NULL IDENTITY
[Lastname_id] [varchar] NOT NULL IDENTITY
[School_id] [varchar]
[Address_id] [varchar]

[Start_date] [datetime] NULL,
[End_date] [datetime] NULL,
[ModifiedDate] [datetime] NOT NULL CONSTRAINT [DF__Identity__]
  ON [PRIMARY]
)
GO

ALTER TABLE [Participants for Invent the Summer].[Identify] ADD CONSTRAINT [PK__Identify__ ]
GO
